    "prepare": "husky install"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "string-template": "^1.0.0"
  },
  "lint-staged": {
    "*.js": "yarn lint",
    "*.{js,css,md,html,json}": "yarn format"
