{
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "eslint": "^9.6.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "neostandard": "^0.11.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "typescript": "^4.3.2 || ^5.0.2 "
  },
  "name": "ts-mommy",
  "version": "0.0.2",
  "description": "Mommy's here to support you when compiling typescript~ ❤️",
  "main": "index.ts",
  "repository": "git@github.com:nyaasophie/ts-mommy.git",
  "author": "Sophie <sophie@sophieis.gay>",
